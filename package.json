{
  "name": "html-to-pdf-action",
  "version": "1.0.0",
  "description": "A Github Action to print HTML files to PDF format using Puppeteer.",
  "main": "dist/index.js",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "pdf-merger-js": "^3.0.5",
    "puppeteer-core": "^1.15.0"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "@types/puppeteer-core": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "@vercel/ncc": "^0.24.1",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "build": "tsc",
    "release": "ncc build src/index.ts",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint **/*.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/southrop/html-to-pdf-action.git"
